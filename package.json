{
  "name": "countre",
  "version": "0.0.1",
  "description": "Click counter with calendar integration",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "start": "NODE_ENV=development concurrently --kill-others-on-fail \"yarn dev:node\" \"yarn dev:react\"",
    "dev:node": "ts-node-dev --respawn --transpileOnly ./app/app.ts",
    "dev:react": "cd client && yarn start",
    "prod": "NODE_ENV=production concurrently --kill-others-on-fail \"yarn prod:node\" \"yarn prod:react\"",
    "prod:node": "tsc & node build/app.js",
    "prod:react": "cd client && yarn build",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "yarn test:node & yarn test:react",
    "test:node": "jest --config ./.jest.config.json",
    "test:react": "cd client && yarn test"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.9.22",
    "node-uuid": "^1.4.8",
    "path": "^0.12.7",
    "swagger-ui-express": "^4.1.4",
    "sync-request": "^6.1.0",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/user-event": "^12.0.11",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/mongoose": "^5.7.30",
    "@types/node": "^14.0.22",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-jest": "^26.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5"
  }
}
